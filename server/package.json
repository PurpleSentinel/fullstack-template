{
  "name": "fullstack-template-server",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/core-api.js",
    "start:auth": "node dist/auth-service.js",
    "start:core": "node dist/core-api.js",
    "start:tools": "node dist/tools-service.js",
    "start:reports": "node dist/reports-service.js",
    "dev:auth": "ts-node-dev --respawn --transpile-only src/auth-service.ts",
    "dev:core": "ts-node-dev --respawn --transpile-only src/core-api.ts",
    "dev:tools": "ts-node-dev --respawn --transpile-only src/tools-service.ts",
    "dev:reports": "ts-node-dev --respawn --transpile-only src/reports-service.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "cookie": "^0.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "otplib": "^12.0.1",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "redis": "^4.7.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cookie": "^0.6.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.17.50",
    "@types/pg": "^8.15.5",
    "@types/qrcode": "^1.5.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
